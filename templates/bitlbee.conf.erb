[settings]
RunMode = <%= run_mode %>
User = <%= user %>
<% unless run_mode == 'Inetd' -%>
DaemonInterface = <%= bind_address %>
DaemonPort = <%= bind_port %>
<% end %>
AuthMode = <%= auth_mode %>
<% if auth_mode == 'Closed' and auth_passwd != :undef -%>AuthPassword = <%= auth_passwd %><% end -%>
OperPassword = <%= oper_passwd %>
<% unless hostname == :undef -%>HostName = <%= hostname %><% end -%>
<% unless config_dir == :undef -%>ConfigDir = <%= config_dir %><% end -%>
<% unless ping_interval == :undef -%>PingInterval = <%= ping_interval %><% end -%>
<% unless ping_timeout == :undef -%>PingTimeOut = <%= ping_timeout %><% end -%>

[defaults]
