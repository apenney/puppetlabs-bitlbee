[settings]
RunMode = <%= run_mode %>
User = <%= user %>
<% unless run_mode == 'Inetd' -%>
DaemonInterface = <%= bind_address %>
DaemonPort = <%= bind_port %>
<% end -%>
AuthMode = <%= auth_mode %>
<% if auth_mode == 'Closed' and auth_passwd != false %>AuthPassword = <%= auth_passwd %><% end %>
OperPassword = <%= oper_passwd %>
<% if hostname -%>HostName = <%= hostname %><% end -%>
<% if config_dir -%>ConfigDir = <%= config_dir %><% end -%>
<% if ping_interval -%>PingInterval = <%= ping_interval %><% end -%>
<% if ping_timeout -%>PingTimeOut = <%= ping_timeout %><% end -%>

[defaults]
